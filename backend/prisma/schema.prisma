generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Definition {
  id        Int      @id @default(autoincrement())
  title     String
  category  String?
  bodyLatex String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
